    "@emotion/styled": "^11.11.0",
    "@eslint/js": "latest",
    "@mui/material": "^5.13.0",
    "@mui/x-data-grid": "^6.4.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vitest/ui": "^4.0.14",
    "drizzle-kit": "^0.31.7",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "^1.17.2",
    "flowbite-qwik": "^1.1.2",
    "globals": "16.4.0",
    "jsreport-studio": "^2.10.1",
    "lucide-qwik": "^1.0.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^4.1.4",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
